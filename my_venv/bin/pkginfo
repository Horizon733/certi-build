#!/home/avinesh/Documents/gith/certibuild/certi-build/my_venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pkginfo==1.7.1','console_scripts','pkginfo'
__requires__ = 'pkginfo==1.7.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pkginfo==1.7.1', 'console_scripts', 'pkginfo')()
    )
